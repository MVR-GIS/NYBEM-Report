@article{prosser_impacts_2018,
	title = {Impacts of {Coastal} {Land} {Use} and {Shoreline} {Armoring} on {Estuarine} {Ecosystems}: an {Introduction} to a {Special} {Issue}},
	volume = {41},
	issn = {1559-2723, 1559-2731},
